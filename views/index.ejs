          $('.navbar').addClass('animate__animated animate__fadeInDown navbar-muncul');
          $('.navbar-brand span').addClass('brand-muncul');
          $('.nav-link b').addClass('link-muncul');
          $('.navbar-toggler.navbar-toggler-icon').addClass('link-muncul');

        } else {
          $('.navbar').removeClass('fadeInDown navbar-muncul navbar-muncul');
          $('.navbar-brand span').removeClass('brand-muncul');
          $('.nav-link b').removeClass('link-muncul');
          $('.navbar-toggler.navbar-toggler-icon').removeClass('link-muncul');
          
          $('.navbar').addClass('animate__animated animate__fadeIn');
        }
      });
    });

